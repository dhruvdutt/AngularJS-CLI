{
  "extends": "airbnb",
  "ecmaVersion": 5,
		"rules": {
			"nonew": 0,
            "no-var": 0,
			"no-eq-null": 2,
			"no-unused-vars": 2,
			"curly": 2,
			"indent": [2, 4],
			"guard-for-in": 2,
			"no-empty": 2,
			"quotes": [2, "single"],
			"eqeqeq": 2,
			"strict": 0,
			"no-undef": 2,
			"no-bitwise": 2,
			"new-cap": 2,
			"wrap-iife": 2,
			"no-native-reassign": 2,
            "prefer-arrow-callback": 0
		},
		"globals": {
            "path": true,
			"require": false,
			"angular": false,
			"describe": false,
			"it": false,
			"before": false,
			"beforeEach": false,
			"after": false,
			"afterEach": false
		}
}
